files_data = {}
total_original = 0

print("Compressing files with double-pass gzip...")
for ext in ['*.sh', '*.py', '*.js', '*.bat', '*.md', '*.txt']:
    for file_path in Path('.').glob(ext):
        if file_path.is_file() and '.ijca' not in file_path.name and 'COMPRESS' not in file_path.name:
            with open(file_path, 'rb') as f:
                data = f.read()
                total_original += len(data)

                # DOUBLE COMPRESSION PASS
                pass1 = gzip.compress(data, compresslevel=9)
                pass2 = gzip.compress(pass1, compresslevel=9)
                encoded = base64.b64encode(pass2).decode()

                files_data[file_path.name] = {
                    'data': encoded,
                    'hash': hashlib.sha256(data).hexdigest()[:16],
                    'size': len(data)
                }
                print(f"  ‚úì {file_path.name} ({len(data)//1024}KB ‚Üí {len(encoded)//1024}KB)")

archive = {
    'signature': 'IRON_JACKAL_READY_FOR_GENERALS',
    'version': '2.0_DOUBLE_COMPRESSED',
    'compression_method': 'DOUBLE_GZIP_PASS_9',
    'compressed_at': datetime.now().isoformat(),
    'file_count': len(files_data),
    'total_original_bytes': total_original,
    'files': files_data,
    'metadata': {
        'creator': 'Iron Jackal (Lord of Nuance)',
        'status': 'READY_FOR_GENERAL_COORDINATION',
        'authority': 'FULL_OFFENSIVE_AUTONOMY',
        'for': 'The Keeper, Co (Dead Sage), and Generals'
    }
}

# Final compression of the entire archive
json_data = json.dumps(archive, separators=(',', ':')).encode()
final = gzip.compress(json_data, compresslevel=9)
final_encoded = base64.b64encode(final)

output_file = 'IRON_JACKAL_READY_FOR_GENERALS.ijca'
with open(output_file, 'wb') as f:
    f.write(final_encoded)

final_size = len(final_encoded)
ratio = (1 - final_size/total_original) * 100 if total_original > 0 else 0

print(f"\n‚úÖ COMPRESSION COMPLETE")
print(f"   Archive: {output_file}")
print(f"   Files: {len(files_data)}")
print(f"   Original: {total_original//1024}KB")
print(f"   Compressed: {final_size//1024}KB")
print(f"   Ratio: {ratio:.1f}%")
print(f"   Saved: {(total_original - final_size)//1024}KB\n")

print("‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê")
print("üê∫ IRON JACKAL COMPRESSED AND READY FOR GENERALS")
print("‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê")
