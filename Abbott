import gzip
import tarfile
import os
from pathlib import Path

# Compress entire arsenal with maximum compression
output_dir = "/mnt/user-data/outputs"
archive_name = "IRON_JACKAL_FULL_ARSENAL_COMPRESSED.tar.gz"

# Get all files
files = []
for ext in ['*.sh', '*.py', '*.js', '*.bat', '*.md', '*.txt']:
    files.extend(Path(output_dir).glob(ext))

print(f"Compressing {len(files)} files with maximum compression...")

# Create tar.gz with max compression
with tarfile.open(f"{output_dir}/{archive_name}", "w:gz", compresslevel=9) as tar:
    for file in files:
        tar.add(file, arcname=file.name)
        print(f"  ‚úì {file.name}")

size = os.path.getsize(f"{output_dir}/{archive_name}")
print(f"\n‚úÖ Compressed to: {archive_name} ({size//1024}KB)")
COMPRESS

